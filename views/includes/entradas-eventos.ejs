<div class="px-3 py-2 mb-2  bg-light">
    <div class="row justify-content-between align-items-center">
        <div class="col-12">
            <div class="row">
                <div class="col-10">
                    <h6 class="mb-0">
                        <b><%= entrada.tipoEntrada.nombre %></b>
                    </h6>
                </div>
            </div>
        </div>
        <div class="col-12">
            <div class="row align-items-center">
                <div class="col text-left">
                    <b style="font-size: 14px; font-weight: 700; line-height: normal;">S/ <%= entrada.precio %> </b>
                </div>
                <div class="col-auto text-right ">
                    <input type="hidden" name="tipoEntrada-<%= entrada.tipoEntrada._id %>" value="<%= entrada.tipoEntrada._id %>">
                    <input type="hidden" name="precioEntrada-<%= entrada.tipoEntrada._id %>" value="<%= entrada.precio %>">
                    <input type="number" name="qtyEntrada-<%= entrada.tipoEntrada._id %>">
                </div>
            </div>
        </div>
    </div>
</div>