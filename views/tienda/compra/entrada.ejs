<div class="entrada-wrapper">
    <div class="left-entrada">
        <img src="https://s3.us-west-2.amazonaws.com/joinnus.com/files/joinnus-com-logo.svg" alt="logo" class="logo">

        <img src="https://upload.wikimedia.org/wikipedia/commons/0/0b/QR_code_Wikimedia_Commons_%28URL%29.png" alt="qrcode" class="qrcode">

        <div class="left-event-name"><%= evento.nombre %></div>
    </div>

    <div class="right-entrada">
        <div class="right-event-name">
            <%= evento.nombre %>
        </div>

        <div class="entrada-details">
            <div class="col-6 p-0">
                <div class="d-flex flex-column">
                    Tipo de Entrada
                    <strong><%= entrada.tipoEntrada.nombre %></strong>                    
                </div>

                <div class="d-flex flex-column">
                    Cantidad
                    <strong><%= entrada.cantidad %></strong>                    
                </div>
                
            </div>
            <div class="col-6 p-0">
                <div class="entrada-data py-1"><%= evento.fecha %></div>
                <div class="entrada-data py-1"><%= evento.hora %></div>
                <div class="entrada-data py-1">S/ <%= entrada.precio %></div>
            </div>
        </div>
        
    </div>
    

</div>